
<% if @is_liked %>
$('#like_<%= @question.id%>').html("UnLike");
noty({text: 'Tweet was Liked', type: 'success', timeout: 3500, progressBar: true});

<% else %>

$('#like_<%= @question.id%>').html("Like");
noty({text: 'Tweet was UnLiked', type: 'success', timeout: 3500, progressBar: true});

<% end %>
